<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    app:startDestination="@id/categoryAddFragment">

    <fragment
        android:id="@+id/categoryAddFragment"
        android:name="com.developer.bshapp.fragments.CategoryAddFragment"
        android:label="fragment_category_add"
        tools:layout="@layout/fragment_category_add" >
        <action
            android:id="@+id/action_categoryAddFragment_to_addNewsFragment"
            app:destination="@id/addNewsFragment" />
        <action
            android:id="@+id/action_categoryAddFragment_to_addJobsFragment"
            app:destination="@id/addJobsFragment" />
        <action
            android:id="@+id/action_categoryAddFragment_to_addAdsFragment"
            app:destination="@id/addAdsFragment" />
        <action
            android:id="@+id/action_categoryAddFragment_to_addMatrimonyFragment"
            app:destination="@id/addMatrimonyFragment" />
        <action
            android:id="@+id/action_categoryAddFragment_to_addRealEstateFragment"
            app:destination="@id/addRealEstateFragment" />
    </fragment>
    <fragment
        android:id="@+id/addNewsFragment"
        android:name="com.developer.bshapp.fragments.addNewsFragment"
        android:label="fragment_add_news"
        tools:layout="@layout/fragment_add_news" >
        <action
            android:id="@+id/action_addNewsFragment_to_sentSuccessfullyFragment"
            app:destination="@id/sentSuccessfullyFragment" />
        <action
            android:id="@+id/action_addNewsFragment_to_sentFailedFragment"
            app:destination="@id/sentFailedFragment" />
    </fragment>
    <fragment
        android:id="@+id/sentSuccessfullyFragment"
        android:name="com.developer.bshapp.fragments.SentSuccessfullyFragment"
        android:label="fragment_sent_successfully"
        tools:layout="@layout/fragment_sent_successfully" />
    <fragment
        android:id="@+id/addJobsFragment"
        android:name="com.developer.bshapp.fragments.addJobsFragment"
        android:label="fragment_add_jobs"
        tools:layout="@layout/fragment_add_jobs" >
        <action
            android:id="@+id/action_addJobsFragment_to_sentSuccessfullyFragment"
            app:destination="@id/sentSuccessfullyFragment" />
        <action
            android:id="@+id/action_addJobsFragment_to_sentFailedFragment"
            app:destination="@id/sentFailedFragment" />
    </fragment>
    <fragment
        android:id="@+id/addAdsFragment"
        android:name="com.developer.bshapp.fragments.AddAdsFragment"
        android:label="fragment_add_ads"
        tools:layout="@layout/fragment_add_ads" >
        <action
            android:id="@+id/action_addAdsFragment_to_sentSuccessfullyFragment"
            app:destination="@id/sentSuccessfullyFragment" />
        <action
            android:id="@+id/action_addAdsFragment_to_sentFailedFragment"
            app:destination="@id/sentFailedFragment" />
    </fragment>
    <fragment
        android:id="@+id/sentFailedFragment"
        android:name="com.developer.bshapp.fragments.SentFailedFragment"
        android:label="fragment_sent_failed"
        tools:layout="@layout/fragment_sent_failed" />
    <fragment
        android:id="@+id/addMatrimonyFragment"
        android:name="com.developer.bshapp.fragments.AddMatrimonyFragment"
        android:label="fragment_add_matrimony"
        tools:layout="@layout/fragment_add_matrimony" >
        <action
            android:id="@+id/action_addMatrimonyFragment_to_sentSuccessfullyFragment"
            app:destination="@id/sentSuccessfullyFragment" />
        <action
            android:id="@+id/action_addMatrimonyFragment_to_sentFailedFragment"
            app:destination="@id/sentFailedFragment" />
    </fragment>
    <fragment
        android:id="@+id/addRealEstateFragment"
        android:name="com.developer.bshapp.fragments.AddRealEstateFragment"
        android:label="fragment_add_real_estate"
        tools:layout="@layout/fragment_add_real_estate" >
        <action
            android:id="@+id/action_addRealEstateFragment_to_sentSuccessfullyFragment"
            app:destination="@id/sentSuccessfullyFragment" />
        <action
            android:id="@+id/action_addRealEstateFragment_to_sentFailedFragment"
            app:destination="@id/sentFailedFragment" />
    </fragment>
</navigation>